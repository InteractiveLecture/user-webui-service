        "./app/exerciseWorksheet/exercise-worksheet-controller_test.ts",
        "./app/exerciseWorksheet/exercise-worksheet-module.ts",
        "./app/home/home-controller.ts",
        "./app/home/home-controller_test.ts",
        "./app/home/home-module.ts",
        "./app/lectureDefinitions/interfaces.ts",
        "./app/lectureDefinitions/models.ts",
        "./app/lectureDefinitions/models_test.ts",
        "./app/login/login-controller.ts",
        "./app/login/login-controller_test.ts",
        "./app/login/login-module.ts",
        "./app/moduleOverview/module-overview-controller.ts",
        "./app/moduleOverview/module-overview-controller_test.ts",
        "./app/moduleOverview/module-overview-module.ts",
        "./app/profile/profile-controller.ts",
        "./app/profile/profile-controller_test.ts",
        "./app/profile/profile-module.ts",
        "./app/route-controller.ts",
        "./app/route-controller_test.ts",
        "./app/topicDetails/topic-details-controller.ts",
        "./app/topicDetails/topic-details-controller_test.ts",
        "./app/topicDetails/topic-details-module.ts",
